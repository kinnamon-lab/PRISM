<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="ProductionLogging" status="warn">
  <Appenders>
    <Console name="Console">
      <PatternLayout pattern="%d %c [%-5p] %msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console" />
    </Root>
  </Loggers>
</Configuration>